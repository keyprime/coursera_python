speed = 108
time = 12
dist = speed * time
print(dist)

print(6 + 2)  # 8
print(6 - 2)  # 4
print(6 * 2)  # 12
print(6 / 2)  # 3.0
print(7 / 2)  # 3.5
print(7 // 2)  # 3 Целочисленное деление, отбрасываем дробную часть
print(6 ** 2)  # 36 Возводим число 6 в степень 2
print(7 % 2)  # Получение остатка от деления числа 7 на 2. Результат 7 - 6 = 1
